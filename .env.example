# HuanuCanvas环境配置模板

# 应用基础配置
NODE_ENV=production
HOST=0.0.0.0
PORT=8765

# 前端配置
FRONTEND_PORT=5206

# 数据库配置
DB_PATH=/opt/huanu-canvas/data/huanu-canvas.db

# API密钥配置
GEMINI_API_KEY=your_gemini_api_key_here

# 安全配置
SESSION_SECRET=your_session_secret_here

# 监控配置
MONITOR_PORT=3001
LOG_LEVEL=info

# 文件存储配置
UPLOAD_PATH=/opt/huanu-canvas/uploads
TEMP_PATH=/opt/huanu-canvas/temp
